{
  "version": "1.0.0",
  "project_name": "Momi's Adventure",
  "gsd_settings": {
    "yolo_mode": true,
    "autonomous_execution": true,
    "skip_confirmations": true,
    "auto_commit": false,
    "profile": "quality"
  },
  "tech_stack": {
    "engine": "Godot 4.5",
    "language": "GDScript",
    "target_platforms": ["Windows", "Linux", "macOS"]
  },
  "conventions": {
    "naming": {
      "scripts": "snake_case.gd",
      "scenes": "snake_case.tscn",
      "classes": "PascalCase",
      "signals": "snake_case",
      "constants": "SCREAMING_SNAKE_CASE"
    },
    "architecture": {
      "pattern": "component-based",
      "autoloads_for": "globals_only",
      "signal_pattern": "call_down_signal_up"
    }
  }
}
